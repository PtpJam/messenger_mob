{
  "cli": {
    "version": ">= 12.5.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_HOST": "http://192.168.0.108:3000",
        "SOCKET": "http://192.168.0.108:3002"
      }
    },
    "preview": {
      "developmentClient": false,
